{
  "name": "@sonamu-kit/ui",
  "version": "0.4.1",
  "description": "SonamuUI â€” Interactive UI Server Toolkit for Sonamu",
  "scripts": {
    "build": "rm -rf dist build && yarn build:client && yarn build:node",
    "build:client": "vite build",
    "build:node": "tsup --config ./tsup.config.js",
    "watch:node": "tsc -w -p ./node/tsconfig.node.json",
    "dev:client": "DEBUG_UI=true vite",
    "dev:node": "tsc-watch -p ./node/tsconfig.node.json --onSuccess \"node -r source-map-support/register dist/index.js run\"",
    "dev": "DEBUG_UI=true run-p dev:node dev:client"
  },
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "build",
    "vite.config.ts",
    "openai.instructions.md"
  ],
  "license": "MIT",
  "author": {
    "name": "Minsang Kim",
    "email": "minsangk@me.com",
    "url": "https://twitter.com/minsangk"
  },
